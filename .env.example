# OpenAI GPT
OPENAI_API_KEY=your_openai_key
OPENAI_MODEL=gpt-4o-mini
OPENAI_BASE_URL=https://api.openai.com/v1

# DeepSea Coder
DEEPSEA_API_KEY=your_deepsea_key
DEEPSEA_MODEL=deepsea-coder-latest
DEEPSEA_BASE_URL=https://api.deepsea.example/v1

